<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.loveqh.dao.UserProjectDao">

    <resultMap id="userProjectResultMap" type="UserProject">
        <id property="id" column="id"></id>
        <result property="userId" column="user_id"></result>
        <result property="projectId" column="project_id"></result>
    </resultMap>

    <select id="getAllProjectsIdOfUser" parameterType="int" resultType="int">
        select * from user_project where user_id=#{value}
    </select>

</mapper>